{
  "name": "kaadon/jwt",
  "type": "library",
  "description": "kaadon jwt for PHP",
  "keywords": [
    "Jwt",
    "Jwttoken",
    "token",
    "kaadon"
  ],
  "homepage": "https://developer.kaadon.com/",
  "license": "Apache-2.0",
  "require": {
    "PHP": ">=8.1",
    "firebase/php-jwt": "^v6.10.1",
    "ext-redis": "*"
  },
  "authors": [
    {
      "name": "kaadon",
      "email": "kaadon@foxmail.com",
      "homepage": "https://www.kaadon.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kaadon\\Jwt\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "think":{
      "config":{
        "jwt": "src/config/jwt.php"
      }
    }
  },
  "require-dev": {
    "topthink/framework": "^8"
  }
}
